{
	"author": "Richard <richardo2016@gmail.com>",
	"name": "@fxjs/sql-ddl-sync",
	"version": "0.1.0",
	"description": "Fibjs SQL DDL Synchronization",
	"keywords": [
		"sql",
		"ddl",
		"sync",
		"mysql",
		"postgres",
		"sqlite"
	],
	"license": "MIT",
	"repository": "http://github.com/fxjs-modules/sql-ddl-sync.git",
	"types": "@types/index.d.ts",
	"main": "./lib",
	"scripts": {
		"build": "fib-typify src -o lib",
		"test-db": "fibjs test/run-db",
		"test-db-sqlite": "cross-env URI=sqlite:test.db npm run test-db",
		"test-db-mysql": "cross-env URI=mysql://root:@localhost:3306/sql-ddl-sync npm run test-db",
		"test": "fibjs test/run",
		"ci": "npm run build && npm run test && npm run test-db-sqlite",
		"prepublishOnly": "npm run build"
	},
	"engines": {
		"fibjs": "*"
	},
	"analyse": false,
	"dependencies": {
		"@fxjs/sql-query": "^0.1.0"
	},
	"devDependencies": {
		"@fibjs/ci": "^2.1.0",
		"@fxjs/orm": "^1.6.10",
		"@types/fibjs": "^0.26.5",
		"cross-env": "^5.2.0",
		"fib-typify": "^0.5.0",
		"should": "^13.2.3"
	},
	"ci": {
		"type": "travis, appveyor",
		"version": [
			"0.21.0",
			"0.22.0",
			"0.23.0",
			"0.24.0",
			"0.25.0",
			"0.26.0",
			"0.26.1"
		]
	}
}
